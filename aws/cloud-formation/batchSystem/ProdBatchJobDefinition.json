      {
        "name": "http.proxyPort",
        "value": "3128"
      },
      {
        "name": "http.nonProxyHosts",
        "value": "172.17.*|localhost|127.*"
      },
      {
        "name": "bonecp.jdbcUrl",
        "value": "jdbc:postgresql://rdspg13prod.chw6qe2cqnfv.eu-west-1.rds.amazonaws.com:5432/digiroad"
