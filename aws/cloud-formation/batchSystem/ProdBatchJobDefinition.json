      },
      {
        "name": "caching",
        "value": "false"
      },
      {
        "name": "cacheHostname",
        "value": "prodroadlinkcache.qrjvyl.cfg.euw1.cache.amazonaws.com"
      },
      {
        "name": "cacheHostPort",
        "value": "11211"
      },
      {
        "name": "cacheTTL",
